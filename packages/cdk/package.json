{
	"name": "@awsless/cdk",
	"version": "0.0.1",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/awsless/awsless.git"
	},
	"bugs": {
		"url": "https://github.com/awsless/awsless/issues"
	},
	"files": [
		"dist"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"test": "pnpm exec code test",
		"synth": "pnpm exec cdk synthesize --app='ts-node --prefer-ts-exts examples/index.ts'",
		"build": "pnpm exec tsup src/index.ts --format cjs,esm --dts --clean",
		"publish": "pnpm run build; pnpm publish --access=public"
	},
	"dependencies": {
		"aws-cdk-lib": "^2.64.0",
		"constructs": "^10.1.246"
	},
	"devDependencies": {
		"aws-cdk": "^2.64.0",
		"ts-node": "^10.9.1"
	}
}
