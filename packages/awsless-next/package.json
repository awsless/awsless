{
	"name": "@awsless/awsless",
	"version": "0.0.535",
	"license": "MIT",
	"type": "module",
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/awsless/awsless.git"
	},
	"bugs": {
		"url": "https://github.com/awsless/awsless/issues"
	},
	"files": [
		"dist"
	],
	"bin": "./dist/bin.js",
	"module": "./dist/server.js",
	"types": "./dist/server.d.ts",
	"exports": {
		".": {
			"import": "./dist/server.js",
			"types": "./dist/server.d.ts"
		},
		"./client": {
			"import": "./dist/client.js",
			"types": "./dist/client.d.ts"
		}
	},
	"scripts": {
		"test": "pnpm vitest",
		"term": "zsh -c 'node dist/bin.js ${*} --config-file=./test/_data/app.jsonc' --",
		"berm": "zsh -c 'pnpm build; pnpm build-features; pnpm build-json-schema; node dist/bin.js ${*} --config-file=./test/_data/app.jsonc' --",
		"build": "pnpm tsup src/server.ts src/client.ts src/bin.ts --format esm --dts --clean",
		"build-json-schema": "pnpm tsup cli/build-json-schema.ts --format esm; node ./dist/build-json-schema.js",
		"build-test-setup": "pnpm tsup src/test/test-global-setup.ts --format esm",
		"build-prebuild": "pnpm tsup src/prebuild.ts --format esm; node ./dist/prebuild.js",
		"build-layers": "mkdir -p dist/layers && cp -r layers/* dist/layers",
		"jack": "pnpm tsup ./example/jack/jack.ts --format esm; node ./dist/jack.js",
		"kennedy": "pnpm tsup ./example/kennedy/kennedy.ts --format esm; node ./dist/kennedy.js",
		"prepublish": "if pnpm test; then pnpm build; pnpm build-json-schema; pnpm build-test-setup; pnpm build-prebuild; pnpm build-layers; else exit; fi"
	},
	"vitest": {
		"include": [
			"./test/**"
		]
	},
