{
	"name": "@awsless/i18n",
	"version": "0.0.5",
	"license": "MIT",
	"type": "module",
	"keywords": [
		"vite",
		"plugin",
		"internationalization",
		"i18n",
		"svelte",
		"sveltekit"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/awsless/awsless.git"
	},
	"bugs": {
		"url": "https://github.com/awsless/awsless/issues"
	},
	"files": [
		"dist"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./svelte": {
			"require": "./dist/svelte.cjs",
			"import": "./dist/svelte.js",
			"types": "./dist/svelte.d.ts"
		}
	},
	"scripts": {
		"test": "pnpm code test",
		"build": "pnpm tsup src/index.ts --format cjs,esm --dts --clean",
		"build-svelte": "pnpm tsup src/framework/svelte.ts --format cjs,esm --dts",
		"prepublish": "if pnpm test; then pnpm build; pnpm build-svelte; else exit; fi"
	},
	"vitest": {
		"exclude": [
			"test/_site"
		]
	},
	"devDependencies": {
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.19",
		"svelte-preprocess": "^6.0.2",
		"vite": "^5.4.2"
	},
	"peerDependencies": {
		"svelte": "^4.2.19",
		"vite": "^5.4.2"
	},
	"dependencies": {
		"@swc/core": "^1.3.70",
		"@types/line-column": "^1.0.2",
		"estree-walker": "^3.0.3",
		"glob": "^10.3.9",
		"line-column": "^1.0.2",
		"openai": "^4.57.0",
		"swc-walk": "1.0.0-rc.2",
		"zod": "^3.21.4"
	}
}
