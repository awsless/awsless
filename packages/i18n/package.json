{
	"name": "@awsless/i18n",
	"version": "0.0.14",
	"license": "MIT",
	"type": "module",
	"keywords": [
		"vite",
		"plugin",
		"internationalization",
		"i18n",
		"svelte",
		"sveltekit",
		"ai"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/awsless/awsless.git"
	},
	"bugs": {
		"url": "https://github.com/awsless/awsless/issues"
	},
	"files": [
		"dist"
	],
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./svelte": {
			"svelte": "./dist/svelte-5.svelte.js",
			"types": "./dist/svelte-5.svelte.d.ts"
		}
	},
	"scripts": {
		"test": "pnpm code test",
		"build": "pnpm tsup src/index.ts --format esm --dts --clean",
		"build-svelte": "pnpm tsup src/framework/svelte-5.svelte.ts --format esm --dts",
		"prepublish": "if pnpm test; then pnpm build; pnpm build-svelte; else exit; fi"
	},
	"vitest": {
		"exclude": [
			"test/_site"
		]
	},
	"devDependencies": {
		"@ai-sdk/openai": "^2.0.42",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.19",
		"svelte-preprocess": "^6.0.2",
		"vite": "^7.1.9"
	},
	"peerDependencies": {
		"svelte": "^4.2.19",
		"vite": "^7.1.9"
	},
	"dependencies": {
		"@swc/core": "^1.3.70",
		"@types/chunk": "^0.0.0",
		"@types/line-column": "^1.0.2",
		"ai": "^5.0.60",
		"chunk": "^0.0.3",
		"estree-walker": "^3.0.3",
		"glob": "^10.3.9",
		"line-column": "^1.0.2",
		"magic-string": "^0.30.18",
		"swc-walk": "1.0.0-rc.2",
		"zod": "^4.1.11"
	}
}
